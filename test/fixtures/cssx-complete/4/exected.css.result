@keyframes ball-animation {
  0%  : [object Object];
  100%: [object Object];
}
@-webkit-keyframes ball-animation {
  0%  : [object Object];
  100%: [object Object];
}
@-moz-keyframes ball-animation {
  0%  : [object Object];
  100%: [object Object];
}
@-o-keyframes ball-animation {
  0%  : [object Object];
  100%: [object Object];
}
.ball {
  animation: ball-animation 1s ease infinite alternate;
}
